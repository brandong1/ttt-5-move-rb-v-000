#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [ " ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"
board = gets.strip
move(board,0,"X")
input = gets.strip
move(board, input, "X")

# come back to this later

